{
  "parameters": {
    "FortiManagerIP": {
      "type": "string",
      "defaultValue": "",
      "metadata": {
        "description": "Select product.",
        "required": "True",
        "subtype": "ip",
        "opt": "-i"
      }
    },
    "Username": {
      "type": "string",
      "defaultValue": "admin",
      "metadata": {
        "description": "Admin username",
        "required": "True",
        "opt": "-u"
      }
    },
    "Password": {
      "type": "string",
      "metadata": {
        "description": "FortiManager admin password",
        "required": "True",
        "subtype": "password",
        "opt": "-p"
      }
    },
    "CSVFile": {
      "type": "string",
      "metadata": {
        "description": "CSV file containing all data for ZTP",
        "subtype": "file",
        "filename": "ztp.xls",
        "required": "True",
        "opt": "-f"
      }
    }
  }
}